{
  "viewName":"iam_organizations_view",
  "query":"SELECT ORGANIZATIONS.ORG_ID AS ORG_ID, ORGANIZATIONS.ORG_NAME AS ORG_NAME, 
ORGANIZATIONS.ORG_DESCRIPTION AS ORG_DESCRIPTION, ORGANIZATIONS.ORG_MASTER_ID AS ORG_MASTER_ID, MASTER.ORG_NAME AS ORG_MASTER_NAME
FROM IAM_ORGANIZATIONS AS ORGANIZATIONS
LEFT JOIN IAM_ORGANIZATIONS AS MASTER ON ORGANIZATIONS.ORG_MASTER_ID = MASTER.ORG_ID"
}
